msgstr "Nó chứa đầy nước bẩn"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_EMPTY.DIRTY_ICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_EMPTY.DIRTY_ICE"
msgid "It's filled with dirty ice"
msgstr "Nó chứa đầy đá bẩn"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_EMPTY.EMPTY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_EMPTY.EMPTY"
msgid "I can fill it with water. Or something else."
msgstr "Tôi có thể đổ đầy nước vào. Hoặc thứ gì khác."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_EMPTY.FULL
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_EMPTY.FULL"
msgid "It's full of rainwater"
msgstr "Trời đầy nước mưa"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_EMPTY.FULL_ICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_EMPTY.FULL_ICE"
