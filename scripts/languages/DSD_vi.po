msgstr "Nó chứa đầy băng"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_STEEL_EMPTY.DIRTY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_STEEL_EMPTY.DIRTY"
msgid "It's filled with dirty water"
msgstr "Nó chứa đầy nước bẩn"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_STEEL_EMPTY.DIRTY_ICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_STEEL_EMPTY.DIRTY_ICE"
msgid "It's filled with dirty ice"
msgstr "Nó chứa đầy đá bẩn"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_STEEL_EMPTY.EMPTY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_STEEL_EMPTY.EMPTY"
msgid "It's a bucket made of iron."
msgstr "Đó là một cái xô làm bằng sắt."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_STEEL_EMPTY.FULL
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_STEEL_EMPTY.FULL"
msgid "It's full of rainwater"
msgstr "Trời đầy nước mưa"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_STEEL_EMPTY.FULL_ICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_STEEL_EMPTY.FULL_ICE"
msgid "It's filled with ice"
msgstr "Nó chứa đầy băng"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_WOODIE_EMPTY.DIRTY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_WOODIE_EMPTY.DIRTY"
msgid "It's filled with dirty water"
msgstr "Nó chứa đầy nước bẩn"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_WOODIE_EMPTY.DIRTY_ICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_WOODIE_EMPTY.DIRTY_ICE"
msgid "It's filled with dirty ice"
msgstr "Nó chứa đầy đá bẩn"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_WOODIE_EMPTY.EMPTY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_WOODIE_EMPTY.EMPTY"
msgid "It's roughly cut inside."
msgstr "Nó bị cắt thô bên trong."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_WOODIE_EMPTY.FULL
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_WOODIE_EMPTY.FULL"
msgid "It's full of rainwater"
msgstr "Nó chứa đầy nước mưa"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_WOODIE_EMPTY.FULL_ICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUCKET_WOODIE_EMPTY.FULL_ICE"
msgid "It's filled with ice"
msgstr "Nó chứa đầy băng"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUTTERBEER
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.BUTTERBEER"
msgid "Definitely not beer, maybe scotch candy?"
msgstr "Chắc chắn không phải bia, có thể là kẹo hạch nhân?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CACTUSFLOWER_TEA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CACTUSFLOWER_TEA"
msgid "A tea from a prickly plant."
msgstr "Trà từ cây đầy gai."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CACTUS_TEA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CACTUS_TEA"
msgid "Hopefully it has no spine about it."
msgstr "Hy vọng là nó không có vấn đề gì."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAFFEINBERRY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAFFEINBERRY"
msgid "It roots itself deep in the ground."
msgstr "Nó cắm rễ sâu vào lòng đất."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAFFEINBERRY_BEAN
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAFFEINBERRY_BEAN"
msgid "Not like coffee beans I used to know."
msgstr "Không giống hạt cà phê tôi từng biết."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAFFEINBERRY_BEAN_COOKED
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAFFEINBERRY_BEAN_COOKED"
msgid "Heat certainly improved it."
msgstr "Nhiệt độ chắc chắn đã cải thiện nó."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAFFEINBERRY_JUICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAFFEINBERRY_JUICE"
msgid "Smells energizing!"
msgstr "Mùi tràn đầy năng lượng!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPDESALINATOR.BOILING_LONG
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPDESALINATOR.BOILING_LONG"
msgid "This is going to take a while to purified."
msgstr "Việc này sẽ mất một thời gian để thanh lọc."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPDESALINATOR.BOILING_SHORT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPDESALINATOR.BOILING_SHORT"
msgid "It's almost done!"
msgstr "Sắp xong rồi!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPDESALINATOR.DONE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPDESALINATOR.DONE"
msgid "It's all done purified."
msgstr "Tất cả đã được thanh lọc xong."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPDESALINATOR.GENERIC
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPDESALINATOR.GENERIC"
msgid "It's no use of it alone."
msgstr "Chỉ riêng nó thôi cũng không có ích gì."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPDESALINATOR.STOP
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPDESALINATOR.STOP"
msgid "It's still too salty."
msgstr "Vẫn còn mặn quá."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPDESALINATOR_ITEM
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPDESALINATOR_ITEM"
msgid "Now I just need a fire."
msgstr "Bây giờ tui cần một ngọn lửa."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPKETTLE.BOILING_LONG
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPKETTLE.BOILING_LONG"
msgid "This is going to take a while to boil."
msgstr "Sẽ mất một lúc để đun sôi."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPKETTLE.BOILING_SHORT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPKETTLE.BOILING_SHORT"
msgid "Almost boiling!"
msgstr "Sắp sôi!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPKETTLE.DONE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPKETTLE.DONE"
msgid "It's all done boiling."
msgstr "Sôi xong rồi."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPKETTLE.GENERIC
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPKETTLE.GENERIC"
msgid "It's no use of it alone."
msgstr "Chỉ riêng nó thôi cũng không có ích gì."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPKETTLE.STOP
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPKETTLE.STOP"
msgid "It's still dirty."
msgstr "Nó vẫn bẩn."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPKETTLE_ITEM
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CAMPKETTLE_ITEM"
msgid "Now I just need a fire."
msgstr "Bây giờ tui cần một ngọn lửa."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CARROT_TEA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CARROT_TEA"
msgid "I hope it's acceptable."
msgstr "Tôi hy vọng nó có thể chấp nhận được."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CHERRY_BLOOM_MADHU
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CHERRY_BLOOM_MADHU"
msgid "This is going on my bottle collection shelf."
msgstr "Cái này đang nằm trên kệ bộ sưu tập chai của tôi."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CHERRY_BLOOM_RUM
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CHERRY_BLOOM_RUM"
msgid "I can just watch the leaves float forever."
msgstr "Tôi có thể ngắm những chiếc lá trôi mãi mãi."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CHERRY_DOCTOR_COLA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CHERRY_DOCTOR_COLA"
msgid "A doctor didn't make this!"
msgstr "Không phải bác sĩ đã làm ra thứ này!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CHERRY_HONEY_TEA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CHERRY_HONEY_TEA"
msgid "I can just taste the flowers."
msgstr "Tôi chỉ có thể nếm những cánh hoa."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CHERRY_ROSEBERRIES_TEA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CHERRY_ROSEBERRIES_TEA"
msgid "The rosey aroma is strong."
msgstr "Hương hoa hồng nồng nàn."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.COCONUT_JUICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.COCONUT_JUICE"
msgid "Is that squishy jelly I see?"
msgstr "Tôi thấy thứ thạch sền sệt đó phải không?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.COCONUT_WINE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.COCONUT_WINE"
msgid "I feel like I'm in the Mediterraneans."
msgstr "Tôi cảm thấy như đang ở Địa Trung Hải."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.COLA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.COLA"
msgid "It tastes like it's infused with science"
msgstr "Nó có vị đậm chất khoa học"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.COLAQUANTUM
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.COLAQUANTUM"
msgid "There's something very scientific about it!"
msgstr "Có điều gì đó rất khoa học về nó!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CORN_BEER
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CORN_BEER"
msgid "According to the science, This is not exactly beer."
msgstr "Theo khoa học, rằng đây không hẳn là bia."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.CORN_WHISKEY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.CORN_WHISKEY"
msgid "They used to make corn whiskey like this in America."
msgstr "Họ từng làm rượu whisky ngô như thế này ở Mỹ."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.DARK_RUINCOLATE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.DARK_RUINCOLATE"
msgid "You should have called me if you're conducting a science experiment."
msgstr "Đáng lẽ bạn nên gọi cho tôi nếu bạn đang tiến hành một thí nghiệm khoa học."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.DESALINATOR.BURNT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.DESALINATOR.BURNT"
msgid "Fire has beaten water."
msgstr "Lửa đánh bại nước."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.DESALINATOR.EMPTY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.DESALINATOR.EMPTY"
msgid "Separating water out of sea water, that's just science."
msgstr "Tách nước ra khỏi muối, đó mới là khoa học."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.DESALINATOR.HASSALT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.DESALINATOR.HASSALT"
msgid "It's all salty."
msgstr "Tất cả đều mặn."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.DESALINATOR.HASWATER
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.DESALINATOR.HASWATER"
msgid "The water's purified."
msgstr "Nước đã được lọc sạch."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.DESALINATOR.PURIFY_LONG
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.DESALINATOR.PURIFY_LONG"
msgid "This's still too salty."
msgstr "Nó vẫn quá mặn."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.DESALINATOR.PURIFY_SHORT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.DESALINATOR.PURIFY_SHORT"
msgid "It's almost done!"
msgstr "Sắp xong rồi!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.DISINFECTANT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.DISINFECTANT"
msgid "I should wash my lab equipment with this."
msgstr "Tôi nên rửa thiết bị phòng thí nghiệm của mình bằng cái này."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.DISTILLERS.DISTILLING_SHORT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.DISTILLERS.DISTILLING_SHORT"
msgid "The distillation is almost over."
msgstr "Quá trình chưng cất gần như kết thúc."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.DISTILLERS.DISTILLTING_LONG
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.DISTILLERS.DISTILLTING_LONG"
msgid "I have to wait until the distillation is over."
msgstr "Tôi phải đợi cho đến khi quá trình chưng cất kết thúc."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.DISTILLERS.DONE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.DISTILLERS.DONE"
msgid "I can't wait to try it."
msgstr "Tôi nóng lòng muốn thử nó."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.DISTILLERS.EMPTY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.DISTILLERS.EMPTY"
msgid "I need to make ethanol for myself."
msgstr "Tôi cần tự sản xuất ethanol."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.DRAGONJUICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.DRAGONJUICE"
msgid "It's still weird."
msgstr "Nó vẫn lạ sao sao ý."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.DUG_CAFFEINBERRY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.DUG_CAFFEINBERRY"
msgid "I should to move that to the ground!"
msgstr "Tôi nên !"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.DUG_RUINCACAO_TREE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.DUG_RUINCACAO_TREE"
msgid "I can plant it somewhere else."
msgstr "Tôi có thể trồng nó ở nơi khác."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.DUG_TEA_TREE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.DUG_TEA_TREE"
msgid "Now it can be replanted in other place."
msgstr "Bây giờ nó có thể được trồng lại ở nơi khác."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.FIG_JUICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.FIG_JUICE"
msgid "Non-Newtonian fluid out of Newton's fig."
msgstr "Chất lỏng phi Newton từ dạng Newton."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.FRUITJUICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.FRUITJUICE"
msgid "It's all fruity."
msgstr "Tất cả đều làm từ trái cây."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.FRUITSODA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.FRUITSODA"
msgid "Sweet, sour, and savory!"
msgstr "Ngọt, chua và mặn!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.FUER
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.FUER"
msgid "It smells quite good."
msgstr "Nó có mùi khá thơm."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.GIANT_BLUEBERRY_GIN
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.GIANT_BLUEBERRY_GIN"
msgid "It's reacting strongly with the spices."
msgstr "Nó phản ứng mạnh với gia vị."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.GIANT_BLUEBERRY_WINE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.GIANT_BLUEBERRY_WINE"
msgid "The fruit is destabilizing during fermenation."
msgstr "Trái cây không ổn định trong quá trình lên men."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.GLOWBERRYJUICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.GLOWBERRYJUICE"
msgid "It's not that bad."
msgstr "Không tệ đến thế đâu."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.GLOWBERRYWINE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.GLOWBERRYWINE"
msgid "It's glowing by science."
msgstr "Nó phát sáng nhờ khoa học."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.GLOWBERRY_GIN
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.GLOWBERRY_GIN"
msgid "Couldn't get any worse making a hard liquor shine."
msgstr "Không thể tệ hơn được nữa khi làm rượu mạnh tỏa sáng."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.GOOPYDRINK
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.GOOPYDRINK"
msgid "I'm not sure if I could drink it."
msgstr "Tôi không chắc mình có thể uống được nó không."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.GREENTEA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.GREENTEA"
msgid "Does it count as a tea?"
msgstr "Nó có được tính là trà không?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.HIBISCUSTEA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.HIBISCUSTEA"
msgid "Beside that aroma, it's too sour!"
msgstr "Chua loẹt, chả bù cho hương thơm của nó!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.HOLE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.HOLE"
msgid "Looks good for me to build a well."
msgstr "Có vẻ tốt cho tôi khi xây một cái giếng."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.HOTRUINCOLATE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.HOTRUINCOLATE"
msgid "A bit of marshmellow would've done."
msgstr "Rắc thêm một chút kẹo dẻo marshmallow sẽ khiến nó hoàn hảo hơn"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.KAHLUA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.KAHLUA"
msgid "Seems like rum brewed with coffee."
msgstr "Có vẻ như rượu rum được pha với cà phê."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.BOILING_LONG
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.BOILING_LONG"
msgid "This is going to take a while."
msgstr "Việc này sẽ mất một lúc."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.BOILING_SHORT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.BOILING_SHORT"
msgid "Almost done!"
msgstr "Sắp xong!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.BURNT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.BURNT"
msgid "It boiled over and burned"
msgstr "Ấm đun nước đã được đun sôi."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.DONE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.DONE"
msgid "Mmm, it's tea time!"
msgstr "Mmm, đến giờ uống trà rồi!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.EMPTY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.EMPTY"
msgid "It makes me thirsty just to look at it."
msgstr "Chỉ nhìn thôi cũng khiến tôi khát nước."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.HASWATER
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.HASWATER"
msgid "We have water, what should we use for it?"
msgstr "Chúng ta có nước, chúng ta nên dùng nước gì?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.MELT_LONG
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.MELT_LONG"
msgid "This is going to take a while to melt."
msgstr "Cái này sẽ mất một thời gian để tan chảy."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.MELT_SHORT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.MELT_SHORT"
msgid "Almost molten!"
msgstr "Gần như nóng chảy!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.PURIFY_LONG
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.PURIFY_LONG"
msgid "This is going to take a while to boil."
msgstr "Sẽ mất một lúc để đun sôi."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.PURIFY_SHORT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.KETTLE.PURIFY_SHORT"
msgid "Almost boiling!"
msgstr "Gần sôi!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.KUMIS
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.KUMIS"
msgid "Wasn't milk supposed to be made into yogurt?"
msgstr "Không phải sữa được dùng làm sữa chua sao?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.LEMONLIMESODA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.LEMONLIMESODA"
msgid "Dentists would likely be horrified if they saw this."
msgstr "Nha sĩ sẽ cảm thấy khiếp sợ khi trông thấy chúng"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.LOTUSTEA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.LOTUSTEA"
msgid "It would be nice to have some biscuits."
msgstr "Thật tuyệt nếu thêm một ít bánh quy."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.LUMPY_BEER
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.LUMPY_BEER"
msgid "The aroma is so sharp!"
msgstr "Mùi thơm quá!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.LUMPY_TEA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.LUMPY_TEA"
msgid "Doesn't smell like dirt..."
msgstr "Không có mùi bụi bẩn..."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.LUMPY_VODKA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.LUMPY_VODKA"
msgid "If i really want to get drunk fast."
msgstr "Nếu như tôi muốn chìm đắm trong niềm say khướt, Vâng!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.LUMPY_WINE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.LUMPY_WINE"
msgid "It's a clean and neat liquor."
msgstr "Đó là một loại rượu sạch và tinh khiết."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.MADHU
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.MADHU"
msgid "The honey is fermented."
msgstr "Mật ong được lên men."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.MADHU_RUM
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.MADHU_RUM"
msgid "I should bring a bottle for the boat."
msgstr "Tôi nên mang theo một chai cho thuyền."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.MIXFLOWER
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.MIXFLOWER"
msgid "How flowery."
msgstr "Thật hoa mỹ."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.MOONWAY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.MOONWAY"
msgid "I think it's photoprotein perserved by alcohol "
msgstr "Tôi nghĩ đó là photoprotein được bảo tồn bằng rượu "

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.MOON_BLOSSOM_TEA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.MOON_BLOSSOM_TEA"
msgid "The aroma sure is an eye-opener."
msgstr "Mùi hương chắc chắn làm bạn mở mang tầm mắt."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.MULLED
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.MULLED"
msgid "A respite for my brain."
msgstr "Một khoảng thời gian nghỉ ngơi cho bộ não của tôi."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.NOBLEWINE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.NOBLEWINE"
msgid "It's beyond wine!"
msgstr "Nó còn hơn cả rượu!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.NUT_COCONUT_BRANDY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.NUT_COCONUT_BRANDY"
msgid "I think I'll be seasick if I have one more drink."
msgstr "Tôi nghĩ tôi sẽ say sóng nếu uống thêm một ly nữa."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.NUT_CORN_WHISKEY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.NUT_CORN_WHISKEY"
msgid "Combined peanuts with whiskey for snacks!"
msgstr "Đậu phộng kết hợp với rượu whisky cho bữa ăn nhẹ!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.ORANGESODA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.ORANGESODA"
msgid "How fanta-stic!"
msgstr "Thật tuyệt vời!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.ORANGE_JUICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.ORANGE_JUICE"
msgid "Orange you glad I brought some juice?"
msgstr "Này cam, có vẻ bạn đang vui vì tôi đã mang theo một ít nước ép?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.PALE_BEER
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.PALE_BEER"
msgid "What a pale drink."
msgstr "Thức uống nhạt quá."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.PALE_WHISKEY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.PALE_WHISKEY"
msgid "The whiskey's color is scientifically impossible."
msgstr "Màu của rượu whisky là không thể tin được về mặt khoa học."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.PETALS_BERRY_BRANDY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.PETALS_BERRY_BRANDY"
msgid "A brandy with a flowery aroma?"
msgstr "Rượu mạnh có mùi thơm hoa?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.PINEANANASSODA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.PINEANANASSODA"
msgid "It's a pine in my mouth."
msgstr "Đó là một cây thông trong miệng tôi."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.PINEANANAS_JUICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.PINEANANAS_JUICE"
msgid "At least the skin won't scratch my mouth anymore."
msgstr "Ít nhất thì da của nó sẽ không làm xước miệng tôi nữa."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.POMEGRANATE_JUICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.POMEGRANATE_JUICE"
msgid "Never take the juicy sweetness for granate."
msgstr "Không bao giờ lấy vị ngọt mọng nước cho granate."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.BOILING_LONG
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.BOILING_LONG"
msgid "The tea is going to take a while."
msgstr "Trà sẽ mất một lúc."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.BOILING_SHORT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.BOILING_SHORT"
msgid "It'll be ready in no-time!"
msgstr "Nó sẽ sẵn sàng ngay lập tức!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.DONE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.DONE"
msgid "Now it's ready to drink!"
msgstr "Bây giờ nó đã sẵn sàng để uống!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.EMPTY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.EMPTY"
msgid "It seems empty, I bet."
msgstr "Tôi cá là nó trống rỗng."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.GENERIC
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.GENERIC"
msgid "Now I can have a tea everywhere!"
msgstr "Bây giờ tôi có thể uống trà ở muôn nơi!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.HASWATER
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.HASWATER"
msgid "We have water, what should we use for it?"
msgstr "Chúng ta có nước, chúng ta nên nó để làm gì?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.MELT_LONG
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.MELT_LONG"
msgid "This is going to take a while to melt."
msgstr "Nó sẽ mất một thời gian để tan chảy."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.MELT_SHORT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.MELT_SHORT"
msgid "Almost molten!"
msgstr "Gần như tan chảy!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.PURIFY_LONG
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.PURIFY_LONG"
msgid "This is going to take a while to boil."
msgstr "Sẽ mất một lúc để đun sôi."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.PURIFY_SHORT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.PORTABLEKETTLE_ITEM.PURIFY_SHORT"
msgid "Almost done!"
msgstr "Sắp xong!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.PULQUE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.PULQUE"
msgid "A traditional drink from the new land?"
msgstr "Thức uống truyền thống từ vùng đất mới?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.QUARTZ_SHAKE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.QUARTZ_SHAKE"
msgid "Hope it doesn't taste like fish."
msgstr "Hope it doesn't taste like fish."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RAINBOW_SHAKE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RAINBOW_SHAKE"
msgid "Thankfully, it lacks flavor."
msgstr "Ơn trời, nó nhạt toẹt"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RICE_SPIRITS
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RICE_SPIRITS"
msgid "Of course it can be distilled ."
msgstr "Tất nhiên là có thể chưng cất."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RICE_WINE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RICE_WINE"
msgid "Is it possible to make fermented liquor from this?"
msgstr "Có thể làm rượu lên men bằng thứ này thật sao?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCACAO
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCACAO"
msgid "It's drier than I thought."
msgstr "Nó khô hơn tôi nghĩ."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCACAO_BEAN
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCACAO_BEAN"
msgid "Too greasy to eat."
msgstr "Ăn quá nhiều dầu mỡ."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCACAO_BEAN_COOKED
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCACAO_BEAN_COOKED"
msgid "생각보다 스모키한데?"
msgstr "생각보다 스모키한데?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCACAO_CREAM_LIQUEUR
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCACAO_CREAM_LIQUEUR"
msgid "It's a creamy chocolatey drink."
msgstr "Đó là đồ uống có vị kem sô-cô-la."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCACAO_SEED
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCACAO_SEED"
msgid "It's germinating already?"
msgstr "Nó đã nảy mầm rồi à?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCACAO_SEED_SAPLING
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCACAO_SEED_SAPLING"
msgid "It'll grow into a tree in time."
msgstr "Nó sẽ phát triển thành cây theo thời gian."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCACAO_TREE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCACAO_TREE"
msgid "That tree is a little creepy."
msgstr "Cái cây đó hơi đáng sợ."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCACAO_WINE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCACAO_WINE"
msgid "Is it possible to make wine with cacao?"
msgstr "Thực sự có thể làm rượu wine từ ca cao hửm?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCHA_LATTE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCHA_LATTE"
msgid "This fluid is the result of a nuclear reaction from the two beans."
msgstr "Thứ dung dịch này là sản phẩm của phản ứng hạt nhân giữa 2 hạt đỗ."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCOLATE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCOLATE"
msgid "I'll need sugar supplements to experiment."
msgstr "Tôi sẽ cần bổ sung đường để thử nghiệm."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCOLATE_SMOOTHIE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINCOLATE_SMOOTHIE"
msgid "Sweet and easy to drink."
msgstr "Ngọt và dễ uống."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINXOCOLATL
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUINXOCOLATL"
msgid "Somewhat acrid"
msgstr "Hơi chát"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUIN_SCHOKAKOLA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUIN_SCHOKAKOLA"
msgid "Who thought of keeping chocolates and shoe polish in a can?"
msgstr "Ai nghĩ đến việc đựng sôcôla và xi đánh giày trong hộp?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUMCHATA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.RUMCHATA"
msgid "Smooth going down."
msgstr "Đi xuống êm ả."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.SEAWEED_SHAKE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.SEAWEED_SHAKE"
msgid "Why was this even made into a drink??"
msgstr "Tại sao thứ này lại được pha thành đồ uống??"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.SODA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.SODA"
msgid "So refreshing!"
msgstr "Thật sảng khoái!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.SPARKLINGWINE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.SPARKLINGWINE"
msgid "I have an overwhelming urge to shake it."
msgstr "Tôi rất muốn lắc nó."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.SPICE_CAFFEINPEPPER
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.SPICE_CAFFEINPEPPER"
msgid "A true spice of life."
msgstr "Một gia vị đích thực của cuộc sống."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.SPOILED_DRINK
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.SPOILED_DRINK"
msgid "It's a muck of spoiled drink."
msgstr "Đó là một đống đồ uống hư hỏng."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.STRAWBERRYSODA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.STRAWBERRYSODA"
msgid "It's a berry straw-ong carbonated drink."
msgstr "Đó là một loại đồ uống có ga có ga từ quả mọng."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.STRAWBERRY_MILK_SMOOTHIE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.STRAWBERRY_MILK_SMOOTHIE"
msgid "It's so berry smoothie."
msgstr "Nó thật là sinh tố quả mọng!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.STRAWBERRY_SMOOTHIE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.STRAWBERRY_SMOOTHIE"
msgid "Where did the straw go?"
msgstr "Ống hút của tôi đâu?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.SUSHIBISCUS
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.SUSHIBISCUS"
msgid "This ought to be a scientifc impossibili-tea."
msgstr "Đây hẳn là một loại trà mà khoa học không thể với tới."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.SWEET_RICE_DRINK
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.SWEET_RICE_DRINK"
msgid "There was a drink like this?"
msgstr "Đó là sự kết hợp mới."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEALEAVES
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEALEAVES"
msgid "It doesn't seem like any tea I know."
msgstr "Có vẻ như nó không giống bất kỳ loại trà nào mà tôi biết."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEALEAVES_DRIED
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEALEAVES_DRIED"
msgid "Now it seems like some tea I know."
msgstr "Bây giờ có vẻ như tôi biết một số loại trà."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEA_LUMPY_VODKA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEA_LUMPY_VODKA"
msgid "Did this come from steeping tea leaves?"
msgstr "Có phải điều này đến từ việc ngâm lá trà?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEA_RUINCOLATE_LATTE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEA_RUINCOLATE_LATTE"
msgid "Well, this was unexpected."
msgstr "Chà, điều này thật bất ngờ."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEA_SEED
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEA_SEED"
msgid "I think there are seeds inside."
msgstr "Tôi nghĩ có hạt bên trong."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEA_SEED_COOKED
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEA_SEED_COOKED"
msgid "Take that, tiny bush!"
msgstr "Lấy cái đó đi, bụi cây nhỏ bé!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEA_SEED_SAPLING
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEA_SEED_SAPLING"
msgid "It will grow into a little tree."
msgstr "Nó sẽ phát triển thành một cái cây nhỏ."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEA_TREE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEA_TREE"
msgid "What a lovely little tree."
msgstr "Thật là một cái cây nhỏ đáng yêu."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEQUILA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.TEQUILA"
msgid "There's a strong rooty smell."
msgstr "Có mùi rễ cây nồng nặc."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.TOFFEE_LATTE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.TOFFEE_LATTE"
msgid "Is it mixed coffee with nuts?"
msgstr "Có phải cà phê pha với các loại hạt không?"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.TOMATO_JUICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.TOMATO_JUICE"
msgid "A cup full of science."
msgstr "Một tách đầy khoa học."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.VEGGIE_TEA
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.VEGGIE_TEA"
msgid "It's all veggie-y."
msgstr "Tất cả đều là rau."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WATERMELON_JUICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WATERMELON_JUICE"
msgid "What a melon-derful juice!"
msgstr "Thật là thức uống dưa hấu tuyệt hảo!!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WATER_CLEAN
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WATER_CLEAN"
msgid "It seems safe to drink."
msgstr "Uống có vẻ an toàn."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WATER_CLEAN_ICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WATER_CLEAN_ICE"
msgid "Ice to meet you so much."
msgstr "Thật tuyệt khi được gặp bạn."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WATER_DIRTY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WATER_DIRTY"
msgid "I don't think it's safe to drink as it is."
msgstr "Tôi không nghĩ uống như vậy là an toàn."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WATER_DIRTY_ICE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WATER_DIRTY_ICE"
msgid "Dirty to meet you."
msgstr "Thật bẩn khi gặp bạn."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WATER_SALTY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WATER_SALTY"
msgid "It's oversalty to drink."
msgstr "Quá mặn để uống."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL"
msgid "Now I can conveniently get clean, fresh water!"
msgstr "Bây giờ tôi có thể thuận tiện lấy nước sạch, trong lành!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_BURYING_KIT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_BURYING_KIT"
msgid "I just wanted to know how deep the hole was."
msgstr "Tôi chỉ muốn biết cái hố sâu bao nhiêu."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_DRILLING
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_DRILLING"
msgid "It's quite a drilling."
msgstr "Thật là khó khăn."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_DRILLING_ITEM
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_DRILLING_ITEM"
msgid "It digs a hole to draw water."
msgstr "Nó đào hố để lấy nước."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_KIT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_KIT"
msgid "I'll need a water hole."
msgstr "Tôi cần một hố nước."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_SPRINKLER.LOWFUEL
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_SPRINKLER.LOWFUEL"
msgid "I should refuel it."
msgstr "Tôi nên thêm nguyên liệu cho nó hoạt động."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_SPRINKLER.OFF
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_SPRINKLER.OFF"
msgid "It stopped working."
msgstr "Nó ngừng hoạt động."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_SPRINKLER.ON
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_SPRINKLER.ON"
msgid "Sprinkle on!"
msgstr "Rắc lên!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_SPRINKLER_KIT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_SPRINKLER_KIT"
msgid "Just be careful of soaking hair."
msgstr "Chỉ cần cẩn thận khi ngâm tóc."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_WATERPUMP.HIGH_PRESSURE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_WATERPUMP.HIGH_PRESSURE"
msgid "The water's going to pour like a fountain."
msgstr "Nước sắp đổ như suối."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_WATERPUMP.LOW_PRESSURE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_WATERPUMP.LOW_PRESSURE"
msgid "The vapor pressure is too low!"
msgstr "Áp suất hơi quá thấp!"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_WATERPUMP.MIDDLE_PRESSURE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_WATERPUMP.MIDDLE_PRESSURE"
msgid "The pressure isn't too bad."
msgstr "Áp lực không quá tệ."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_WATERPUMP.RECHARG_PRESSURE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_WATERPUMP.RECHARG_PRESSURE"
msgid "There isn't even any water for vapor..."
msgstr "Thậm chí không có nước để tạo hơi..."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_WATERPUMP_KIT
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WELL_WATERPUMP_KIT"
msgid "It's the age of carrying water...with water."
msgstr "Đã đến thời đại gánh nước...bằng nước."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WHEAT_BEER
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WHEAT_BEER"
msgid "Scientifically speaking, it's wheat beer."
msgstr "Nói một cách khoa học thì đó là bia lúa mì."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WHEAT_WHISKEY
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WHEAT_WHISKEY"
msgid "It's a type of grain whiskey."
msgstr "Đó là một loại whisky ngũ cốc."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WHITE_RUINCOLATE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WHITE_RUINCOLATE"
msgid "Too greasy if I have too much."
msgstr "Tôi sẽ bị ngớn nếu ăn quá nhiều"

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WINE
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WINE"
msgid "Don't wine on me."
msgstr "Đừng uống rượu với tôi."

#. STRINGS.CHARACTERS.GENERIC.DESCRIBE.WX78MODULE_NONEDRUNK
msgctxt "STRINGS.CHARACTERS.GENERIC.DESCRIBE.WX78MODULE_NONEDRUNK"
msgid "So much science packed into one tiny gizmo."
msgstr "Rất nhiều khoa học được gói gọn trong một thiết bị nhỏ bé."

#. STRINGS.CHARACTERS.WALTER.ACTIONFAIL.GIVE.WELL_BUSY
msgctxt "STRINGS.CHARACTERS.WALTER.ACTIONFAIL.GIVE.WELL_BUSY"
msgid "The water's still coming up."
msgstr "Nước vẫn đang lên."

#. STRINGS.CHARACTERS.WALTER.ACTIONFAIL.GIVE.WELL_NOTEMPTY
msgctxt "STRINGS.CHARACTERS.WALTER.ACTIONFAIL.GIVE.WELL_NOTEMPTY"
msgid "Have to remove the bucket first."
msgstr "Tôi cần loại bỏ cái xô trước đã"

#. STRINGS.CHARACTERS.WALTER.ANNOUNCE_CAFFINE_BUFF_START
msgctxt "STRINGS.CHARACTERS.WALTER.ANNOUNCE_CAFFINE_BUFF_START"
msgid "I wanna start exploring right now."
msgstr "Tôi muốn bắt đầu khám phá ngay bây giờ."

#. STRINGS.CHARACTERS.WALTER.ANNOUNCE_CAFFINE_BUFF_STOP
msgctxt "STRINGS.CHARACTERS.WALTER.ANNOUNCE_CAFFINE_BUFF_STOP"
msgid "Getting a little tired..."
msgstr "Hơi mệt một chút..."

#. STRINGS.CHARACTERS.WALTER.ANNOUNCE_CURE_WATERBORNE
msgctxt "STRINGS.CHARACTERS.WALTER.ANNOUNCE_CURE_WATERBORNE"
msgid "I'm feeling better now."
msgstr "Bây giờ tôi thấy khỏe hơn rồi."

#. STRINGS.CHARACTERS.WALTER.ANNOUNCE_DCAPACITY_CRITICAL
msgctxt "STRINGS.CHARACTERS.WALTER.ANNOUNCE_DCAPACITY_CRITICAL"
msgid "Why do I feel so heavy..."
msgstr "Sao tôi thấy nặng nề quá..."

#. STRINGS.CHARACTERS.WALTER.ANNOUNCE_DCAPACITY_HALF
msgctxt "STRINGS.CHARACTERS.WALTER.ANNOUNCE_DCAPACITY_HALF"
msgid "I'm feeling like something's wrong..."
msgstr "Tôi cảm thấy như có điều gì đó không ổn..."

#. STRINGS.CHARACTERS.WALTER.ANNOUNCE_DRUNK
msgctxt "STRINGS.CHARACTERS.WALTER.ANNOUNCE_DRUNK"
msgid "I heard a story on a radio once...hic"
msgstr "Tôi từng nghe một câu chuyện trên radio...hic"

#. STRINGS.CHARACTERS.WALTER.ANNOUNCE_DRUNK_END
msgctxt "STRINGS.CHARACTERS.WALTER.ANNOUNCE_DRUNK_END"
msgid "I feel much better now!"
msgstr "Bây giờ tôi cảm thấy tốt hơn nhiều!"

#. STRINGS.CHARACTERS.WALTER.ANNOUNCE_DRUNK_IMMUNITY
msgctxt "STRINGS.CHARACTERS.WALTER.ANNOUNCE_DRUNK_IMMUNITY"
msgid "only_used_by_wathgrithr"
msgstr ""

#. STRINGS.CHARACTERS.WALTER.ANNOUNCE_NOTCHARGE
msgctxt "STRINGS.CHARACTERS.WALTER.ANNOUNCE_NOTCHARGE"
msgid "Can't milk it, it's utterly dry! Heh, that was good."
msgstr "Không thể vắt sữa được, nó khô quá rồi! Heh, điều đó thật tốt."

#. STRINGS.CHARACTERS.WALTER.ANNOUNCE_NOTHIRSTSIESTA
msgctxt "STRINGS.CHARACTERS.WALTER.ANNOUNCE_NOTHIRSTSIESTA"
msgid "I need a drink before bedtime."
msgstr "Tôi cần uống nước trước khi đi ngủ."

#. STRINGS.CHARACTERS.WALTER.ANNOUNCE_NOTHIRSTSLEEP
msgctxt "STRINGS.CHARACTERS.WALTER.ANNOUNCE_NOTHIRSTSLEEP"
msgid "I'm too thirsty to sleep."
msgstr "Tôi khát quá không ngủ được."

#. STRINGS.CHARACTERS.WALTER.ANNOUNCE_SLEEP_END
msgctxt "STRINGS.CHARACTERS.WALTER.ANNOUNCE_SLEEP_END"
msgid "I feel a bit better after sleeping"
msgstr "Tôi cảm thấy tốt hơn một chút sau khi ngủ"

#. STRINGS.CHARACTERS.WALTER.ANNOUNCE_THIRST
msgctxt "STRINGS.CHARACTERS.WALTER.ANNOUNCE_THIRST"
msgid "How about taking a moment to have some water?"
msgstr "Bạn có muốn dành một chút thời gian để uống nước không?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.ABSINTHE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.ABSINTHE"
msgid "I think I heard it's called the green fairy!"
msgstr "Tôi nghĩ tôi nghe nói nó được gọi là nàng tiên xanh!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.ADDITIVE_DAIRY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.ADDITIVE_DAIRY"
msgid "Is it cake cream?"
msgstr "Có phải là bánh kem không?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.ADDITIVE_NUT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.ADDITIVE_NUT"
msgid "Trail mix! Oh never mind..."
msgstr "Kết hợp đường mòn! Ôi đừng bận tâm..."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.ADDITIVE_PETALS
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.ADDITIVE_PETALS"
msgid "It's dried leaves and petals!"
msgstr "Đó là lá và cánh hoa khô!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.ADDITIVE_SEED
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.ADDITIVE_SEED"
msgid "I think I'll make a sample out of these seeds."
msgstr "Tôi nghĩ tôi sẽ làm mẫu từ những hạt giống này."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.AREUHI
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.AREUHI"
msgid "Milk for grown ups is transparent."
msgstr "Sữa dành cho người lớn có dạng trong suốt."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.ARRACK
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.ARRACK"
msgid "I wish the fruit was in my hand."
msgstr "Tôi ước gì trái cây nằm trong tay mình."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.AUROBOW
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.AUROBOW"
msgid "I wanna drink that color. Maybe just a sip?"
msgstr "Tôi muốn uống màu đó. Có lẽ chỉ một ngụm thôi?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BANANA_JUICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BANANA_JUICE"
msgid "This is better than I thought it'd be."
msgstr "Cái này tốt hơn tôi nghĩ."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BARREL.BURNT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BARREL.BURNT"
msgid "Not my water!!"
msgstr "Không phải nước của tôi!!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BARREL.GENERIC
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BARREL.GENERIC"
msgid "The water will always be clean!"
msgstr "Nước sẽ luôn sạch!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BERRIES_JUICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BERRIES_JUICE"
msgid "Berry berry good! See? It's made out of berries."
msgstr "Quả mọng ngon! Bạn thấy không? Nó được làm từ quả mọng."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BERRY_BRANDY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BERRY_BRANDY"
msgid "Not too sure, but I'm guessing brandy?"
msgstr "Không chắc lắm, nhưng tôi đoán là rượu mạnh?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BERRY_GIN
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BERRY_GIN"
msgid "I heard it was made for medicinal purposes, but it made a lot of people sick."
msgstr "Tôi nghe nói nó được sản xuất với mục đích chữa bệnh nhưng nó đã khiến rất nhiều người bị bệnh."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BLACKTEA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BLACKTEA"
msgid "Which dinosaur enjoys tea? Tea-Rex!"
msgstr "Con khủng long nào thích trà? Tea-Rex!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BLACKTEA_ICED
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BLACKTEA_ICED"
msgid "Ice is in the pot-tea."
msgstr "Có đá trong ấm trà."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BOILBOOK
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BOILBOOK"
msgid "It doesn't hurt to memorize one more handbook."
msgstr "Sẽ không có hại gì khi ghi nhớ thêm một cuốn sổ tay nữa."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BREWERY.BURNT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BREWERY.BURNT"
msgid "It burnt when it was supposed to brew."
msgstr "Nó bị cháy đáng lẽ việc của nó là ủ rượu."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BREWERY.DONE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BREWERY.DONE"
msgid "I think it's done!"
msgstr "Tôi nghĩ là xong rồi!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BREWERY.EMPTY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BREWERY.EMPTY"
msgid "That's a big barrel!"
msgstr "Đó là một cái thùng lớn!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BREWERY.FERMENTING_LONG
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BREWERY.FERMENTING_LONG"
msgid "This'll take long. Come on Woby, let's go play."
msgstr "Việc này sẽ mất nhiều thời gian. Thôi nào Woby, đi chơi thôi."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BREWERY.FERMENTING_SHORT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BREWERY.FERMENTING_SHORT"
msgid "If you listen closely, you can hear the bubbles."
msgstr "Nếu bạn lắng nghe kỹ, bạn có thể nghe thấy tiếng bong bóng."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.DIRTY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.DIRTY"
msgid "I think this went bad!"
msgstr "Tôi nghĩ chuyện này trở nên tệ rồi!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.DIRTY_ICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.DIRTY_ICE"
msgid "I don't think I can eat this ice."
msgstr "Tôi không nghĩ mình có thể ăn được đá."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.EMPTY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.EMPTY"
msgid "Looks like a bucket that has drifted onto a deserted island!"
msgstr "Cảm như một chiếc xô trôi dạt vào đảo hoang vậy!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.FULL
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.FULL"
msgid "Safe to drink now!"
msgstr "An toàn để uống bây giờ!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.FULL_ICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.FULL_ICE"
msgid "Aw, now I have to boil it."
msgstr "Ồ, bây giờ tôi phải đun nó."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_EMPTY.DIRTY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_EMPTY.DIRTY"
msgid "I think this went bad!"
msgstr "Tôi nghĩ chuyện này trở nên tệ rồi!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_EMPTY.DIRTY_ICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_EMPTY.DIRTY_ICE"
msgid "I don't think I can eat this ice."
msgstr "Tôi không nghĩ mình có thể ăn được đá."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_EMPTY.EMPTY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_EMPTY.EMPTY"
msgid "Saving rain water is good for survival!"
msgstr "Tốt nhất nên tiết kiệm nước mưa cho công cuộc sinh tồn này!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_EMPTY.FULL
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_EMPTY.FULL"
msgid "Safe to drink now!"
msgstr "An toàn để uống bây giờ!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_EMPTY.FULL_ICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_EMPTY.FULL_ICE"
msgid "Aw, now I have to boil it."
msgstr "Ồ, bây giờ tôi phải đun nó."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_STEEL_EMPTY.DIRTY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_STEEL_EMPTY.DIRTY"
msgid "I think this went bad!"
msgstr "Tôi nghĩ chuyện này trở nên tệ rồi!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_STEEL_EMPTY.DIRTY_ICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_STEEL_EMPTY.DIRTY_ICE"
msgid "I don't think I can eat this ice."
msgstr "Tôi không nghĩ mình có thể ăn được cục đá nước này."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_STEEL_EMPTY.EMPTY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_STEEL_EMPTY.EMPTY"
msgid "Too small for head gear."
msgstr "Quá nhỏ để đeo đầu."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_STEEL_EMPTY.FULL
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_STEEL_EMPTY.FULL"
msgid "Safe for drinking now!"
msgstr "Bây giờ có thể uống an toàn!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_STEEL_EMPTY.FULL_ICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_STEEL_EMPTY.FULL_ICE"
msgid "Aw, now I have to boil it."
msgstr "Ồ, bây giờ tôi phải đun nó."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_WOODIE_EMPTY.DIRTY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_WOODIE_EMPTY.DIRTY"
msgid "I think this went bad!"
msgstr "Tôi nghĩ chuyện này trở nên tệ rồi!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_WOODIE_EMPTY.DIRTY_ICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_WOODIE_EMPTY.DIRTY_ICE"
msgid "I don't think I can eat this ice."
msgstr "Tôi không nghĩ mình có thể ăn được đá."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_WOODIE_EMPTY.EMPTY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_WOODIE_EMPTY.EMPTY"
msgid "This bucket is wild!"
msgstr "Cái thùng này thật hoang tàn!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_WOODIE_EMPTY.FULL
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_WOODIE_EMPTY.FULL"
msgid "Safe to drink now!"
msgstr "An toàn để uống bây giờ!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_WOODIE_EMPTY.FULL_ICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUCKET_WOODIE_EMPTY.FULL_ICE"
msgid "Aw, now I have to boil it."
msgstr "Ồ, bây giờ tôi phải đun nó."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.BUTTERBEER
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.BUTTERBEER"
msgid "It's the drink the wizard had in that novel!"
msgstr "Đó là đồ uống mà phù thủy đã uống trong cuốn tiểu thuyết đó!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CACTUSFLOWER_TEA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CACTUSFLOWER_TEA"
msgid "This is how you survive in the desert."
msgstr "Đây là cách bạn sống sót trong sa mạc."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CACTUS_TEA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CACTUS_TEA"
msgid "Hope i don't get a thorn in my mouth."
msgstr "Hope i don't get a thorn in my mouth."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CAFFEINBERRY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CAFFEINBERRY"
msgid "I thought coffee grew from trees."
msgstr "Tôi tưởng cà phê mọc từ cây."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CAFFEINBERRY_BEAN
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CAFFEINBERRY_BEAN"
msgid "This is what coffee looks like?"
msgstr "Cà phê trông như thế này à?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CAFFEINBERRY_BEAN_COOKED
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CAFFEINBERRY_BEAN_COOKED"
msgid "Now it smells more like coffee!"
msgstr "Bây giờ nó có mùi giống cà phê hơn!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CAFFEINBERRY_JUICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CAFFEINBERRY_JUICE"
msgid "Drinking too much will be a ca-pain! Heh..."
msgstr "Uống nhiều sẽ bị đau bụng! Heh..."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPDESALINATOR.BOILING_LONG
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPDESALINATOR.BOILING_LONG"
msgid "Do you guys wanna hear a story while we wait?"
msgstr "Các bạn có muốn nghe kể chuyện trong khi chúng tôi chờ đợi không?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPDESALINATOR.BOILING_SHORT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPDESALINATOR.BOILING_SHORT"
msgid "Think it's almost done!"
msgstr "Tôi nghĩ mọi việc gần như đã hoàn tất!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPDESALINATOR.DONE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPDESALINATOR.DONE"
msgid "It won't be salty anymore!"
msgstr "Nó sẽ không còn mặn nữa!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPDESALINATOR.GENERIC
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPDESALINATOR.GENERIC"
msgid "We'll boil the sea water into steam to make plain water."
msgstr "Chúng ta sẽ đun sôi nước biển thành hơi nước để tạo thành nước thường."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPDESALINATOR.STOP
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPDESALINATOR.STOP"
msgid "Let's look for kindling before we get thirsty Woby."
msgstr "Hãy tìm củi trước khi chúng ta khát Woby."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPDESALINATOR_ITEM
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPDESALINATOR_ITEM"
msgid "The handbook said to never drink salt water."
msgstr "Sổ tay nói không được uống nước muối."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPKETTLE.BOILING_LONG
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPKETTLE.BOILING_LONG"
msgid "Better make sure it doesn't boil too much."
msgstr "Tốt hơn hết hãy đảm bảo nó không sôi quá nhiều."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPKETTLE.BOILING_SHORT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPKETTLE.BOILING_SHORT"
msgid "It's boiling!"
msgstr "Nó đang sôi!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPKETTLE.DONE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPKETTLE.DONE"
msgid "Safe to drink now!"
msgstr "An toàn để uống bây giờ!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPKETTLE.GENERIC
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPKETTLE.GENERIC"
msgid "It's safer to boil nature's water."
msgstr "Đun sôi nước thiên nhiên sẽ an toàn hơn."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPKETTLE.STOP
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPKETTLE.STOP"
msgid "Let's look for kindling before we get thirst Woby."
msgstr "Hãy tìm củi trước khi chúng ta khát Woby."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPKETTLE_ITEM
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CAMPKETTLE_ITEM"
msgid "It's never smart to drink something from the wilderness."
msgstr "Không bao giờ là thông minh nếu uống thứ gì đó từ vùng hoang dã."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CARROT_TEA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CARROT_TEA"
msgid "Poor carrot...but not really."
msgstr "Cà rốt tội nghiệp...nhưng không hẳn."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CHERRY_BLOOM_MADHU
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CHERRY_BLOOM_MADHU"
msgid "Looks like alcohol in a gift shop!"
msgstr "Trông giống như rượu trong cửa hàng quà tặng!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CHERRY_BLOOM_RUM
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CHERRY_BLOOM_RUM"
msgid "This will look good behind my bug collection!"
msgstr "Bộ sưu tập lỗi của tôi trông sẽ rất đẹp!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CHERRY_DOCTOR_COLA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CHERRY_DOCTOR_COLA"
msgid "Definitely not a dentist."
msgstr "Chắc chắn không phải là nha sĩ."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CHERRY_HONEY_TEA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CHERRY_HONEY_TEA"
msgid "It's like syrup medicine!"
msgstr "Giống như thuốc sirô!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CHERRY_ROSEBERRIES_TEA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CHERRY_ROSEBERRIES_TEA"
msgid "It's so hip! And relaxed."
msgstr "Thật sành điệu! Và thư giãn."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.COCONUT_JUICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.COCONUT_JUICE"
msgid "This is coco'not'. Why? Because it's not coco'nut', heh heh."
msgstr "Đây là dừa'. Tại sao? Bởi vì nó không phải là dừa', heh heh."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.COCONUT_WINE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.COCONUT_WINE"
msgid "Looks sweet!"
msgstr "Trông thật ngọt ngào!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.COLA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.COLA"
msgid "Who knew I'd drink cola again."
msgstr "Ai biết tôi lại uống cola."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.COLAQUANTUM
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.COLAQUANTUM"
msgid "I can hear it bouncing inside!"
msgstr "Tôi có thể nghe thấy nó nảy bên trong!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CORN_BEER
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CORN_BEER"
msgid "I think it's beer!"
msgstr "Tôi nghĩ đó là bia!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.CORN_WHISKEY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.CORN_WHISKEY"
msgid "Not sure, but I think it's whiskey!"
msgstr "Không chắc lắm, nhưng tôi nghĩ đó là rượu whisky!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.DARK_RUINCOLATE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.DARK_RUINCOLATE"
msgid "I think Mr. Warly made a mistake."
msgstr "Tôi nghĩ ông Warly đã phạm sai lầm."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.DESALINATOR.BURNT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.DESALINATOR.BURNT"
msgid "So this is...burnt."
msgstr "Vậy cái này...cháy rồi."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.DESALINATOR.EMPTY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.DESALINATOR.EMPTY"
msgid "There wasn't anything about purifying salt water in the handbook."
msgstr "Sổ tay không nói gì về việc lọc nước muối."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.DESALINATOR.HASSALT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.DESALINATOR.HASSALT"
msgid "Salt is just extra!"
msgstr "Muối chỉ là chất bổ sung!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.DESALINATOR.HASWATER
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.DESALINATOR.HASWATER"
msgid "No more salt!"
msgstr "Không còn muối nữa!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.DESALINATOR.PURIFY_LONG
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.DESALINATOR.PURIFY_LONG"
msgid "This'll take long. I'll go exploring with Woby."
msgstr "Việc này sẽ mất nhiều thời gian. Tôi sẽ đi khám phá cùng Woby."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.DESALINATOR.PURIFY_SHORT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.DESALINATOR.PURIFY_SHORT"
msgid "I think it's almost done!"
msgstr "Tôi nghĩ mọi việc gần như đã hoàn tất!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.DISINFECTANT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.DISINFECTANT"
msgid "The handbook said disinfectants saved a bunch of lives."
msgstr "Sổ tay nói rằng chất khử trùng đã cứu được rất nhiều sinh mạng."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.DISTILLERS.DISTILLING_SHORT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.DISTILLERS.DISTILLING_SHORT"
msgid "I think it's for alcohol?"
msgstr "Tôi nghĩ đó là rượu?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.DISTILLERS.DISTILLTING_LONG
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.DISTILLERS.DISTILLTING_LONG"
msgid "This'll be a while."
msgstr "Sẽ mất một lúc."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.DISTILLERS.DONE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.DISTILLERS.DONE"
msgid "I should tell the guys at camp drinks are ready."
msgstr "Tôi nên nói với những người ở trại đồ uống đã sẵn sàng."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.DISTILLERS.EMPTY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.DISTILLERS.EMPTY"
msgid "It's got all kinds of complicated gadgets attached!"
msgstr "Nó có đủ loại tiện ích phức tạp kèm theo!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.DRAGONJUICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.DRAGONJUICE"
msgid "Is this...dragon blood?"
msgstr "Đây có phải...máu rồng không?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.DUG_CAFFEINBERRY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.DUG_CAFFEINBERRY"
msgid "Take this to camp, and the grown-ups will love it."
msgstr "Mang cái này đi cắm trại, người lớn sẽ thích nó."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.DUG_RUINCACAO_TREE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.DUG_RUINCACAO_TREE"
msgid "Let's find a place for my own chocolate facory."
msgstr "Hãy tìm một nơi cho mặt tiền sô-cô-la của riêng tôi."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.DUG_TEA_TREE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.DUG_TEA_TREE"
msgid "Let's look for a place to plant Woby!"
msgstr "Hãy tìm nơi trồng Woby!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.FIG_JUICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.FIG_JUICE"
msgid "Not good as a juice either..."
msgstr "Cũng không tốt bằng nước trái cây..."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.FRUITJUICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.FRUITJUICE"
msgid "It's fruit so it's good for me!"
msgstr "Đó là trái cây mọng nên nó tốt cho tôi!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.FRUITSODA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.FRUITSODA"
msgid "Home made fruit soda!"
msgstr "Soda trái cây làm tại nhà!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.FUER
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.FUER"
msgid "How about trying some 'Folli-Tea' for a refreshing taste of nature?"
msgstr "Thử một chút 'Folli-Tea' để thưởng thức hương vị sảng khoái từ thiên nhiên nào!!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.GIANT_BLUEBERRY_GIN
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.GIANT_BLUEBERRY_GIN"
msgid "No firearms!"
msgstr "Không lấy vũ khí!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.GIANT_BLUEBERRY_WINE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.GIANT_BLUEBERRY_WINE"
msgid "Uh, at least that kept its temper."
msgstr "Uh, ít nhất thứ đó giữ được bình tĩnh."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.GLOWBERRYJUICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.GLOWBERRYJUICE"
msgid "I wonder if this'll make me bright?"
msgstr "Tôi tự hỏi liệu điều này có làm tôi tươi sáng không?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.GLOWBERRYWINE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.GLOWBERRYWINE"
msgid "I wonder if this'll make me bright...er?"
msgstr "Tôi tự hỏi liệu điều này có làm tôi tươi sáng...er không?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.GLOWBERRY_GIN
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.GLOWBERRY_GIN"
msgid "Shiney Gin! But it's not in a lamp."
msgstr "Rượu Gin lấp lánh! Nhưng nó không có trong đèn."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.GOOPYDRINK
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.GOOPYDRINK"
msgid "Um...will Woby eat this?"
msgstr "Ừm...Woby sẽ ăn cái này chứ?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.GREENTEA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.GREENTEA"
msgid "I think I saw a bug! Just kidding."
msgstr "I think I saw a bug! Just kidding."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.HIBISCUSTEA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.HIBISCUSTEA"
msgid "I have a feeling it's gonna be sour."
msgstr "Tôi có cảm giác nó sẽ chua."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.HOLE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.HOLE"
msgid "I wonder if it leads to the other side of the world?"
msgstr "Tôi tự hỏi liệu nó có dẫn đến bên kia thế giới không?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.HOTRUINCOLATE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.HOTRUINCOLATE"
msgid "Now I don't have to wait for Christmas."
msgstr "Bây giờ tôi không phải đợi đến Giáng sinh."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.KAHLUA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.KAHLUA"
msgid "I think Mr. Warly wanted to drink coffee and alcohol"
msgstr "Tôi nghĩ ông Warly muốn uống cà phê và rượu"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.BOILING_LONG
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.BOILING_LONG"
msgid "This'll take long. Come on Woby, let's go play."
msgstr "Việc này sẽ mất nhiều thời gian. Thôi nào Woby, đi chơi thôi."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.BOILING_SHORT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.BOILING_SHORT"
msgid "Almost ready!"
msgstr "Gần như sẵn sàng!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.BURNT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.BURNT"
msgid "So this is...burnt."
msgstr "Vậy cái này... bị cháy!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.DONE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.DONE"
msgid "I should tell everyone at camp it's done!"
msgstr "Tôi nên nói với mọi người ở trại rằng mọi việc đã xong!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.EMPTY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.EMPTY"
msgid "3 3 3 rule! You won't last 3 days without water!"
msgstr "Quy tắc 3-3-3! Bạn sẽ không thể tồn tại 3 ngày nếu không có nước!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.HASWATER
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.HASWATER"
msgid "Now let's look for something to brew with Woby!"
msgstr "Bây giờ hãy tìm thứ gì đó để pha với Woby!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.MELT_LONG
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.MELT_LONG"
msgid "Needs to melt more."
msgstr "Cần tan chảy nhiều hơn."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.MELT_SHORT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.MELT_SHORT"
msgid "Almost done melting."
msgstr "Gần như đã tan chảy xong."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.PURIFY_LONG
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.PURIFY_LONG"
msgid "Not finishing is worse."
msgstr "Không hoàn thành còn tệ hơn."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.PURIFY_SHORT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.KETTLE.PURIFY_SHORT"
msgid "We'll survive three days more!"
msgstr "Chúng ta sẽ sống sót thêm ba ngày nữa!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.KUMIS
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.KUMIS"
msgid "I wish it were yogurt."
msgstr "Tôi ước gì đó là sữa chua."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.LEMONLIMESODA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.LEMONLIMESODA"
msgid "Sweet and stingy."
msgstr "Ngọt ngào và keo kiệt."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.LOTUSTEA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.LOTUSTEA"
msgid "Back to where it came from. Hot water."
msgstr "Quay lại nơi nó xuất phát. Nước nóng."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.LUMPY_BEER
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.LUMPY_BEER"
msgid "Veggies even in drinks is good for me!"
msgstr "Rau ngay cả trong đồ uống cũng tốt cho tôi!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.LUMPY_TEA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.LUMPY_TEA"
msgid "Ew... I hope it doesn't smell like dirt."
msgstr "Eo ôi,... tôi mong rằng nó không có vị như đất"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.LUMPY_VODKA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.LUMPY_VODKA"
msgid "Ugh! How do grown ups drink this?"
msgstr "Ugh! Người lớn làm sao uống được thứ này?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.LUMPY_WINE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.LUMPY_WINE"
msgid "Maybe for disinfecting scars on my adventures?"
msgstr "Có thể để khử trùng vết sẹo trong chuyến phiêu lưu của tôi?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.MADHU
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.MADHU"
msgid "Did bees figure out how to brew drinks with honey?"
msgstr "Có phải ong đã tìm ra cách pha đồ uống bằng mật ong không?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.MADHU_RUM
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.MADHU_RUM"
msgid "They say this is how they stored water on boats!"
msgstr "Họ nói đây là cách họ trữ nước trên thuyền!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.MIXFLOWER
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.MIXFLOWER"
msgid "Do bees drink tea? Probably not."
msgstr "Ong có uống trà không? Có lẽ là không."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.MOONWAY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.MOONWAY"
msgid "Seven colors are shining in my palm."
msgstr "Bảy màu sắc tỏa sáng trong lòng bàn tay tôi."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.MOON_BLOSSOM_TEA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.MOON_BLOSSOM_TEA"
msgid "I wanted cheese, not tea."
msgstr "Tôi muốn phô mai, không phải trà."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.MULLED
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.MULLED"
msgid "Bitter is good for you!"
msgstr "Thuốc đắng giã tật"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.NOBLEWINE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.NOBLEWINE"
msgid "This looks sweet. Only if it wasn't alcohol."
msgstr "Món này trông thật ngọt ngào. Chỉ khi đó không phải là rượu."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.NUT_COCONUT_BRANDY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.NUT_COCONUT_BRANDY"
msgid "Pirate drink!"
msgstr "Đồ uống cho cướp biển!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.NUT_CORN_WHISKEY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.NUT_CORN_WHISKEY"
msgid "It would have been better if it was a form that I could eat."
msgstr "Sẽ tốt hơn nếu nó ở dạng mà tôi có thể ăn được."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.ORANGESODA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.ORANGESODA"
msgid "What do you call it when an orange soda dreams? Fanta-sy!"
msgstr "Bạn gọi nó là gì khi trông thấy một cốc soda cam? Fanta-sy :>"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.ORANGE_JUICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.ORANGE_JUICE"
msgid "Meet my 'oranginal' juice."
msgstr "Gặp nước ép 'oranginal' của tôi."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.PALE_BEER
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.PALE_BEER"
msgid "Now I know why grown ups call it pale ail."
msgstr "Bây giờ tôi biết tại sao người lớn gọi nó là bia nhạt."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.PALE_WHISKEY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.PALE_WHISKEY"
msgid "I think it's some type of medicine."
msgstr "Tôi nghĩ đó là một loại thuốc nào đó."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.PETALS_BERRY_BRANDY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.PETALS_BERRY_BRANDY"
msgid "I don't know about the taste, but it looks pretty!"
msgstr "Tôi không biết mùi vị thế nào, nhưng nó trông khá ngon!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.PINEANANASSODA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.PINEANANASSODA"
msgid "We'll call it the Pinetree Pioneer Apple Soda."
msgstr "Chúng tôi sẽ gọi nó là soda Thông Phong Táo."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.PINEANANAS_JUICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.PINEANANAS_JUICE"
msgid "Do you know who Anana's brother is? Banana!"
msgstr "Bạn có biết anh trai của Anana là ai không? Chuối!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.POMEGRANATE_JUICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.POMEGRANATE_JUICE"
msgid "Is this juice from inside a fruit which was already inside another fruit?"
msgstr "Nước ép này có phải từ bên trong một loại trái cây vốn đã ở bên trong một loại trái cây khác không?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.BOILING_LONG
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.BOILING_LONG"
msgid "This'll take long. Come on Woby, let's go play."
msgstr "Việc này sẽ mất nhiều thời gian. Thôi nào Woby, đi chơi thôi."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.BOILING_SHORT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.BOILING_SHORT"
msgid "Almost ready!"
msgstr "Gần như sẵn sàng!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.DONE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.DONE"
msgid "I can have a fancy cup of tea in the wild too!"
msgstr "Tôi cũng có thể thưởng thức một tách trà ưa thích ở nơi hoang dã!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.EMPTY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.EMPTY"
msgid "Think we should find water first."
msgstr "Tôi nghĩ chúng ta nên tìm nước trước."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.GENERIC
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.GENERIC"
msgid "Now we can drink in the wilderness!"
msgstr "Bây giờ chúng ta có thể uống rượu ở nơi hoang dã!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.HASWATER
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.HASWATER"
msgid "Now let's find something to brew with Woby!"
msgstr "Bây giờ hãy tìm thứ gì đó để pha với Woby!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.MELT_LONG
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.MELT_LONG"
msgid "Needs to melt more."
msgstr "Cần tan chảy nhiều hơn."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.MELT_SHORT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.MELT_SHORT"
msgid "Almost done melting."
msgstr "Gần như đã tan chảy xong."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.PURIFY_LONG
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.PURIFY_LONG"
msgid "Not finishing is worse."
msgstr "Không hoàn thành còn tệ hơn."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.PURIFY_SHORT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.PORTABLEKETTLE_ITEM.PURIFY_SHORT"
msgid "Now we can explore for three more days!"
msgstr "Bây giờ chúng ta có thể khám phá thêm ba ngày nữa!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.PULQUE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.PULQUE"
msgid "There wern't instructions in the handbook drinking cactus like this."
msgstr "Không có hướng dẫn nào trong sổ tay cách uống xương rồng như thế này."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.QUARTZ_SHAKE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.QUARTZ_SHAKE"
msgid "Better than fish eyes, right?"
msgstr "Tốt hơn mắt cá phải không?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RAINBOW_SHAKE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RAINBOW_SHAKE"
msgid "It's a delicious color!"
msgstr "Đó là một màu sắc tuyệt vời!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RICE_SPIRITS
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RICE_SPIRITS"
msgid "Ugh, why would anybody like this?"
msgstr "Ugh, sao lại có người thích cái này nhỉ?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RICE_WINE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RICE_WINE"
msgid "Probably not milk."
msgstr "Có lẽ không phải sữa."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCACAO
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCACAO"
msgid "They say it's dwarf treasure!"
msgstr "Họ nói đó là kho báu của người lùn!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCACAO_BEAN
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCACAO_BEAN"
msgid "Wait, chocolate is white...and smells like grease?"
msgstr "Đợi đã, sôcôla có màu trắng...và có mùi dầu mỡ?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCACAO_BEAN_COOKED
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCACAO_BEAN_COOKED"
msgid "This isn't the chocolate I know."
msgstr "Đây không phải là sôcôla mà tôi biết."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCACAO_CREAM_LIQUEUR
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCACAO_CREAM_LIQUEUR"
msgid "Looks tasty! Too bad it's alcohol."
msgstr "Trông ngon quá! Tiếc là lại là rượu."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCACAO_SEED
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCACAO_SEED"
msgid "This will grow into a new tree."
msgstr "Cái này sẽ phát triển thành một cây mới."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCACAO_SEED_SAPLING
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCACAO_SEED_SAPLING"
msgid "It's blooming already."
msgstr "Nó nở rồi."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCACAO_TREE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCACAO_TREE"
msgid "It's a mini chocolate factory!"
msgstr "Đó là một nhà máy sản xuất sôcôla mini!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCACAO_WINE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCACAO_WINE"
msgid "Cocoa for grown ups!"
msgstr "Ca cao dành cho người lớn!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCHA_LATTE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCHA_LATTE"
msgid "Never for Woby!"
msgstr "Không bao giờ dành cho Woby!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCOLATE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCOLATE"
msgid "Oompa- loompa- doompety- doo-."
msgstr "Oompa loompa doompety doo."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCOLATE_SMOOTHIE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINCOLATE_SMOOTHIE"
msgid "Chocolate and ice will be crunchy."
msgstr "Sô cô la và đá sẽ giòn."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINXOCOLATL
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RUINXOCOLATL"
msgid "Is this cocoa? Really?"
msgstr "Đây là cacao hả? Thật đấy à"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RUIN_SCHOKAKOLA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RUIN_SCHOKAKOLA"
msgid "At least the chocolate won't get squashed on my adventures."
msgstr "Ít nhất sô cô la sẽ không bị đè bẹp trong chuyến phiêu lưu của tôi."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.RUMCHATA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.RUMCHATA"
msgid "Looks sweet and soft!"
msgstr "Trông ngọt ngào và mềm mại!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.SEAWEED_SHAKE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.SEAWEED_SHAKE"
msgid "What if it starts growing sea weeds inside me?"
msgstr "Không biết liệu rong biển sẽ \"đâm chồi nảy lộc\" trong bụng tôi không nhỉ?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.SODA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.SODA"
msgid "I've never seen mineral water near camp."
msgstr "Tôi chưa bao giờ thấy nước khoáng gần trại."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.SPARKLINGWINE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.SPARKLINGWINE"
msgid "I wanna shake this!!"
msgstr "Tôi muốn lắc cái này!!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.SPICE_CAFFEINPEPPER
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.SPICE_CAFFEINPEPPER"
msgid "Makes camping food taste much better."
msgstr "Làm cho đồ ăn cắm trại ngon hơn nhiều."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.SPOILED_DRINK
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.SPOILED_DRINK"
msgid "I don't think I should drink this..."
msgstr "Tôi không nghĩ mình nên uống thứ này..."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.STRAWBERRYSODA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.STRAWBERRYSODA"
msgid "I've never seen strawberry soda before!"
msgstr "Tôi chưa bao giờ thấy soda dâu trước đây!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.STRAWBERRY_MILK_SMOOTHIE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.STRAWBERRY_MILK_SMOOTHIE"
msgid "Refreshing and cool!"
msgstr "Sáng mát và sảng khoái!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.STRAWBERRY_SMOOTHIE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.STRAWBERRY_SMOOTHIE"
msgid "Strong but soft. Because it's a strawng berry smoothie."
msgstr "Cường mạnh và mềm lỏng, đó là thức uống sinh tố dâu tằm"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.SUSHIBISCUS
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.SUSHIBISCUS"
msgid "Maybe just one sip?"
msgstr "Có lẽ chỉ một ngụm thôi?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.SWEET_RICE_DRINK
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.SWEET_RICE_DRINK"
msgid "Not a juice, But not a tea!"
msgstr "Không phải nước ép, không phải trà!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.TEALEAVES
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.TEALEAVES"
msgid "Smells grassy."
msgstr "Có mùi cỏ."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.TEALEAVES_DRIED
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.TEALEAVES_DRIED"
msgid "The leaves are rustly."
msgstr "Lá bị rỉ sét."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.TEA_LUMPY_VODKA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.TEA_LUMPY_VODKA"
msgid "This looks healthy! but it's still alcohol."
msgstr "Thứ này trông có vẻ tốt cho sức khỏe! Nhưng nó vẫn là rượu."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.TEA_RUINCOLATE_LATTE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.TEA_RUINCOLATE_LATTE"
msgid "It's green...which I don't like."
msgstr "Nó có màu xanh...mà tôi không thích."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.TEA_SEED
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.TEA_SEED"
msgid "This will grow into a new tree."
msgstr "Cái này sẽ phát triển thành một cây mới."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.TEA_SEED_COOKED
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.TEA_SEED_COOKED"
msgid "We can eat this."
msgstr "Chúng ta có thể ăn cái này."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.TEA_SEED_SAPLING
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.TEA_SEED_SAPLING"
msgid "It's growing."
msgstr "Nó đang phát triển."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.TEA_TREE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.TEA_TREE"
msgid "Nothing about tee treas in the handbook."
msgstr "Không có gì về tee trong sổ tay."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.TEQUILA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.TEQUILA"
msgid "Back to being prickly, huh?"
msgstr "Trở lại gai góc hả?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.TOFFEE_LATTE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.TOFFEE_LATTE"
msgid "It's coffee, but with everything I like!"
msgstr "Đó là cà phê, nhưng với mọi thứ tôi thích!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.TOMATO_JUICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.TOMATO_JUICE"
msgid "This looks like a vampire drink."
msgstr "Thứ này trông giống đồ uống của ma cà rồng."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.VEGGIE_TEA
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.VEGGIE_TEA"
msgid "Um,This looks...healthy."
msgstr "Hừm, thứ này.... trông thật tốt cho sức khoẻ"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WATERMELON_JUICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WATERMELON_JUICE"
msgid "Should I call this watermelon water?"
msgstr "Tôi có nên gọi đây là nước dưa hấu không?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WATER_CLEAN
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WATER_CLEAN"
msgid "Staying hydrated is key to survival!"
msgstr "Luôn đủ nước là chìa khóa để sinh tồn!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WATER_CLEAN_ICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WATER_CLEAN_ICE"
msgid "Oh, gosh,It's frozen."
msgstr "Ôi Chúa ơi, nó bị đóng băng rồi."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WATER_DIRTY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WATER_DIRTY"
msgid "This needs more boiling."
msgstr "Cái này cần đun sôi nhiều hơn."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WATER_DIRTY_ICE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WATER_DIRTY_ICE"
msgid "The ice is so black!."
msgstr "Băng trông đen quá!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WATER_SALTY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WATER_SALTY"
msgid "Drinking sea water will make you more thirsty."
msgstr "Uống nước biển sẽ khiến bạn khát hơn."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL"
msgid "What's wrong Woby? Did somebody fall in?"
msgstr "Sao vậy Woby? Có ai rơi vào à?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_BURYING_KIT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_BURYING_KIT"
msgid "I should fill in any holes that I can't even step down into for safety!"
msgstr "Tôi nên lấp đầy những cái lỗ mà tôi không thể bước xuống để đảm bảo an toàn!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_DRILLING
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_DRILLING"
msgid "One well at a time!"
msgstr "Mỗi lần một giếng!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_DRILLING_ITEM
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_DRILLING_ITEM"
msgid "Wanna dig a well hole Woby?"
msgstr "Muốn đào một cái giếng không em, Woby?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_KIT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_KIT"
msgid "Don't dwell around the well. Heh."
msgstr "Đừng ở quanh giếng. Heh."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_SPRINKLER.LOWFUEL
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_SPRINKLER.LOWFUEL"
msgid "I need to put in more wood."
msgstr "Tôi cần cho thêm gỗ vào."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_SPRINKLER.OFF
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_SPRINKLER.OFF"
msgid "It's taking a break."
msgstr "Đang tạm nghỉ."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_SPRINKLER.ON
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_SPRINKLER.ON"
msgid "That's odd, Woby likes it!"
msgstr "Kỳ lạ đấy, Woby thích nó!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_SPRINKLER_KIT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_SPRINKLER_KIT"
msgid "Let's give our plants some 'irri-gation' now!"
msgstr "Hãy 'tưới nước' cho cây của chúng ta ngay bây giờ!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_WATERPUMP.HIGH_PRESSURE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_WATERPUMP.HIGH_PRESSURE"
msgid "The power of steam is awesome!"
msgstr "Sức mạnh của hơi nước thật tuyệt vời!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_WATERPUMP.LOW_PRESSURE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_WATERPUMP.LOW_PRESSURE"
msgid "Does the machine need more food?"
msgstr "Máy có cần thêm thức ăn không?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_WATERPUMP.MIDDLE_PRESSURE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_WATERPUMP.MIDDLE_PRESSURE"
msgid "It's as good as Mr. Wolfgang!"
msgstr "Giỏi như ông Wolfgang!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_WATERPUMP.RECHARG_PRESSURE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_WATERPUMP.RECHARG_PRESSURE"
msgid "We need to find a pond Woby."
msgstr "Chúng ta cần tìm một cái ao Woby."

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_WATERPUMP_KIT
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WELL_WATERPUMP_KIT"
msgid "High-Draw Pump! It leads the water from low to high?"
msgstr "Máy bơm hút cao! Nó dẫn nước từ thấp lên cao?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WHEAT_BEER
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WHEAT_BEER"
msgid "Looks like real beer!"
msgstr "Trông giống bia thật!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WHEAT_WHISKEY
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WHEAT_WHISKEY"
msgid "This could work as a disinfectant in a jiffy!"
msgstr "Cái này có thể hoạt động như một chất khử trùng trong chốc lát!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WHITE_RUINCOLATE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WHITE_RUINCOLATE"
msgid "Let's save it so that we don't eat too much at once!"
msgstr "Chúng ta hãy tiết kiệm để không ăn quá nhiều cùng một lúc nhé!"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WINE
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WINE"
msgid "Thought it'd be sweet. Why is it so bitter?"
msgstr "Tưởng ngọt. Sao lại đắng thế?"

#. STRINGS.CHARACTERS.WALTER.DESCRIBE.WX78MODULE_NONEDRUNK
msgctxt "STRINGS.CHARACTERS.WALTER.DESCRIBE.WX78MODULE_NONEDRUNK"
msgid "Robot guts! Neat!"
msgstr "Ruột robot! Gọn gàng!"

#. STRINGS.CHARACTERS.WANDA.ACTIONFAIL.GIVE.WELL_BUSY
msgctxt "STRINGS.CHARACTERS.WANDA.ACTIONFAIL.GIVE.WELL_BUSY"
msgid "Did I forget that I put bucket in the well?"
msgstr "Tôi quên mình đã đổ nước vào giếng à?"

#. STRINGS.CHARACTERS.WANDA.ACTIONFAIL.GIVE.WELL_NOTEMPTY
msgctxt "STRINGS.CHARACTERS.WANDA.ACTIONFAIL.GIVE.WELL_NOTEMPTY"
msgid "I need to move this bucket out of the way first."
msgstr "Bạn cần đổ xô rỗng trước đã."

#. STRINGS.CHARACTERS.WANDA.ANNOUNCE_CAFFINE_BUFF_START
msgctxt "STRINGS.CHARACTERS.WANDA.ANNOUNCE_CAFFINE_BUFF_START"
msgid "Excellent, I feel like I can chase time!"
msgstr "Tuyệt vời, tôi cảm thấy mình có thể đuổi theo thời gian!"

#. STRINGS.CHARACTERS.WANDA.ANNOUNCE_CAFFINE_BUFF_STOP
msgctxt "STRINGS.CHARACTERS.WANDA.ANNOUNCE_CAFFINE_BUFF_STOP"
msgid "Darn it! Time is speeding up again!"
msgstr "Chết tiệt! Thời gian lại trôi nhanh quá!"

#. STRINGS.CHARACTERS.WANDA.ANNOUNCE_CURE_WATERBORNE
msgctxt "STRINGS.CHARACTERS.WANDA.ANNOUNCE_CURE_WATERBORNE"
msgid "I finally fixed my internal clock."
msgstr "Cuối cùng tôi đã sửa được chiếc đồng hồ bỏ túi."

#. STRINGS.CHARACTERS.WANDA.ANNOUNCE_DCAPACITY_CRITICAL
msgctxt "STRINGS.CHARACTERS.WANDA.ANNOUNCE_DCAPACITY_CRITICAL"
msgid "Time is accelerating!"
msgstr "Thời gian đang tăng tốc!"

#. STRINGS.CHARACTERS.WANDA.ANNOUNCE_DCAPACITY_HALF
msgctxt "STRINGS.CHARACTERS.WANDA.ANNOUNCE_DCAPACITY_HALF"
msgid "I'm starting to feel more composed as the intoxication wears off."
msgstr "Tôi bắt đầu cảm thấy bình tĩnh hơn khi cơn say đã qua đi."

#. STRINGS.CHARACTERS.WANDA.ANNOUNCE_DRUNK
msgctxt "STRINGS.CHARACTERS.WANDA.ANNOUNCE_DRUNK"
msgid "Oh botheration, I feel so dizzy!"
msgstr "Ồ, tôi cảm thấy chóng mặt quá!"

#. STRINGS.CHARACTERS.WANDA.ANNOUNCE_DRUNK_END
msgctxt "STRINGS.CHARACTERS.WANDA.ANNOUNCE_DRUNK_END"
msgid "No more time to waste!"
msgstr "Không còn thời gian để lãng phí nữa!"

#. STRINGS.CHARACTERS.WANDA.ANNOUNCE_NOTCHARGE
msgctxt "STRINGS.CHARACTERS.WANDA.ANNOUNCE_NOTCHARGE"
msgid "At present, that doesn't have milk."
msgstr "Hiện tại tôi không mang theo sữa."

#. STRINGS.CHARACTERS.WANDA.ANNOUNCE_NOTHIRSTSIESTA
msgctxt "STRINGS.CHARACTERS.WANDA.ANNOUNCE_NOTHIRSTSIESTA"
msgid "I should find something to drink first."
msgstr "Tôi nên tìm thứ gì đó để uống trước đã."

#. STRINGS.CHARACTERS.WANDA.ANNOUNCE_NOTHIRSTSLEEP
msgctxt "STRINGS.CHARACTERS.WANDA.ANNOUNCE_NOTHIRSTSLEEP"
msgid "I should find something to drink first."
msgstr "Tôi nên tìm thứ gì đó để uống trước đã."

#. STRINGS.CHARACTERS.WANDA.ANNOUNCE_SLEEP_END
msgctxt "STRINGS.CHARACTERS.WANDA.ANNOUNCE_SLEEP_END"
msgid "Oh my, which time phase am I in now?"
msgstr "Ôi trời, bây giờ tôi đang ở giai đoạn nào rồi?"

#. STRINGS.CHARACTERS.WANDA.ANNOUNCE_THIRST
msgctxt "STRINGS.CHARACTERS.WANDA.ANNOUNCE_THIRST"
msgid "I wonder how long it's been since I had a drink?"
msgstr "Không biết đã bao lâu rồi tôi chưa uống nước nhỉ?"

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.ABSINTHE
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.ABSINTHE"
msgid "There's no time to sip it slowly."
msgstr "Không có thời gian để nhấm nháp nó một cách chậm rãi."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.ADDITIVE_DAIRY
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.ADDITIVE_DAIRY"
msgid "When it's easier to drink, I'll drink it faster."
msgstr "Khi nó dễ uống hơn, tôi sẽ uống nhanh hơn."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.ADDITIVE_NUT
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.ADDITIVE_NUT"
msgid "Should I just pop it in my mouth?"
msgstr "Tôi có nên cho nó vào miệng không?"

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.ADDITIVE_PETALS
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.ADDITIVE_PETALS"
msgid "Bundles of old petals yearning for timeless beauty."
msgstr "Bó cánh hoa già khao khát vẻ đẹp vượt thời gian."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.ADDITIVE_SEED
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.ADDITIVE_SEED"
msgid "Fledgling seeds turned spice shall brew potent spirits."
msgstr "Những loại thảo mộc này mơ ước một ngày nào đó sẽ được đóng chai."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.AREUHI
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.AREUHI"
msgid "it won't spoil quickly now."
msgstr "Tôi hy vọng bây giờ nó sẽ không bị hỏng nhanh."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.ARRACK
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.ARRACK"
msgid "Roaming the islands, free from the constraints of time."
msgstr "Vượt qua các hòn đảo, thoát khỏi sự ràng buộc của thời gian."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.AUROBOW
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.AUROBOW"
msgid "I'm not sure if this is possible, but anyway, this is awesome!"
msgstr "Đó là một tinh linh có khả năng chống chịu cầu vồng."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BANANA_JUICE
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BANANA_JUICE"
msgid "it doesn't go down my throat easily when it's warm."
msgstr "Nó làm bạn ấm lên khi trời nóng."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BARREL.BURNT
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BARREL.BURNT"
msgid "Who could have predicted that a barrel with water would catch fire?"
msgstr "Ai có thể đoán trước được thùng nước sẽ bốc cháy?"

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BARREL.GENERIC
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BARREL.GENERIC"
msgid "At least I don't have to worry about the water going bad."
msgstr "Ít nhất tôi không phải lo lắng về việc nước bị hỏng."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BERRIES_JUICE
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BERRIES_JUICE"
msgid "Isn't it faster to just pick and eat them?"
msgstr "Không phải chỉ cần hái và ăn chúng là nhanh hơn sao?"

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BERRY_BRANDY
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BERRY_BRANDY"
msgid "Whiskey, Brandy... Honestly, I don't know the difference, as long as it doesn't spoil."
msgstr "Whiskey, Brandy... Thành thật mà nói, tôi không biết sự khác biệt, miễn là nó không bị hỏng."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BERRY_GIN
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BERRY_GIN"
msgid "Just one drink should be enough."
msgstr "Chỉ cần một ly là đủ."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BLACKTEA
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BLACKTEA"
msgid "Maybe I can indulge in a treat once in a while?"
msgstr "Có lẽ thỉnh thoảng tôi có thể thưởng thức một món gì đó?"

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BLACKTEA_ICED
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BLACKTEA_ICED"
msgid "A sip of something cold is nice."
msgstr "Một ngụm đồ lạnh thật tuyệt."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BOILBOOK
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BOILBOOK"
msgid "When do I have time to think about refreshments when I'm even pressed for time to boil water!"
msgstr "Ai có thời gian nghĩ đến trà khi đã có nước đun sôi?"

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BREWERY.BURNT
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BREWERY.BURNT"
msgid "I suppose this was its destiny."
msgstr "Tôi cho rằng đây là định mệnh của nó."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BREWERY.DONE
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BREWERY.DONE"
msgid "Finally, something to show for it!"
msgstr "Cuối cùng cũng có thứ để trưng bày!"

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BREWERY.EMPTY
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BREWERY.EMPTY"
msgid "This is a real time hog."
msgstr "Việc sản xuất bia cần có thời gian tuyệt vời."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BREWERY.FERMENTING_LONG
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BREWERY.FERMENTING_LONG"
msgid "Fermentation takes time, and I can't just wait around."
msgstr "Quá trình lên men cần có thời gian và tôi không thể chỉ chờ đợi được."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BREWERY.FERMENTING_SHORT
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BREWERY.FERMENTING_SHORT"
msgid "I've been waiting for quite a while!"
msgstr "Tôi đã đợi khá lâu rồi!"

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.DIRTY
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.DIRTY"
msgid "Oh botheration, it's been contaminated by time."
msgstr "Ôi trời, nó bị ô nhiễm theo thời gian rồi."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.DIRTY_ICE
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.DIRTY_ICE"
msgid "Oh botheration, it's frozen while still contaminated."
msgstr "Ôi trời, nó bị đóng băng trong khi vẫn bị ô nhiễm."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.EMPTY
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.EMPTY"
msgid "I'm surprised that something made of old wood is more durable, huh."
msgstr "Nó chỉ là một cái xô bình thường thôi."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.FULL
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.FULL"
msgid "Let's quickly retrieve this before its condition changes."
msgstr "Hãy lấy nó trước khi thủy triều thay đổi!"

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.FULL_ICE
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BUCKET_DRIFTWOOD_EMPTY.FULL_ICE"
msgid "Oh botheration, it's frozen over from the passage of time."
msgstr "Ôi trời, nó đã đóng băng theo thời gian rồi."

#. STRINGS.CHARACTERS.WANDA.DESCRIBE.BUCKET_EMPTY.DIRTY
msgctxt "STRINGS.CHARACTERS.WANDA.DESCRIBE.BUCKET_EMPTY.DIRTY"
